{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,svg}",
      "/css/*",
      "/js/*",
      "/assets/*",
      "*.{css,js,png,jpg,gif,svg,ico,woff,woff2,ttf,eot}"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "application/json"
  }
}
